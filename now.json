{
    "version":2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@now/node@canary"
        }
    ],
    "routes":[
        {
            "src":"/about/",
            "status":200,
            "dest":"/"
        },
        {
            "src":"/getregusers",
            "status":200, 
            "dest":"/"
        },
        {
            "src":"/labs",
            "methods":["Get"],
            "dest":"/"
        },
        {
            "src":"/attended-workshop",
            "methods":["POST"],
            "status":200,
            "dest":"/"
        },
        {
            "src":"/register",
            "methods":["POST"],
            "status":200,
            "dest":"/"
        },
        {
            "src":"/register/spotreg",
            "methods":["POST"],
            "status":200,
            "dest":"/"
        },
        {
            "src":"/confirm",
            "methods":["POST"],
            "status":200,
            "dest":"/"
        }
    ]
}